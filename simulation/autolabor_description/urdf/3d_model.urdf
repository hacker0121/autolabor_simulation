<?xml version="1.0"?>
<robot name="autolabor_description">
  <joint name="dummy_joint" type="fixed">
    <parent link="base_link"/>
    <child link="body"/>
    <origin xyz="0 0 0.124" rpy="0 0 0"/>
  </joint>
  <joint name="cam_joint" type="fixed">
    <parent link="body"/>
    <child link="camera"/>
    <origin xyz="-0.7 0 0.5" rpy="0 1.570796327 0"/>
  </joint>
  <joint name="ray_joint" type="fixed">
    <parent link="body"/>
    <child link="ray"/>
    <origin xyz="0 0 1.236" rpy="0 0 0"/>
  </joint>
  <joint name="fljoint" type="continuous">
    <parent link="body"/>
    <child link="flw"/>
    <origin rpy="0 0 0" xyz="-0.15 -0.275 0"/>
    <axis xyz="0 -1 0"/>
  </joint>
  <joint name="frjoint" type="continuous">
    <parent link="body"/>
    <child link="frw"/>
    <origin rpy="0 0 0" xyz="-0.15 0.275 0"/>
    <axis xyz="0 -1 0"/>
  </joint>
  <joint name="cjoint" type="fixed">
    <parent link="body"/>
    <child link="caster"/>
    <origin rpy="0 0 0" xyz="0.221 0 -0.086"/>
  </joint>

  <link name="base_link"/>
  <link name="body">
    <inertial>
      <mass value="30"/>
      <origin rpy="0  0  0" xyz="-0.000006 -0.000009 0.176904"/>
      <inertia ixx="3.545e+12" ixy="-1e+9" ixz="-2.84e+11" iyy="4.553e+12" iyz="-3e+9" izz="3.081e+12"/>
    </inertial>
    <collision>
      <origin rpy="0  0  0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/body.stl"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/body.stl"/>
      </geometry>
    </visual>
  </link>
  <link name="flw">
    <inertial>
      <mass value="10"/>
      <origin rpy="0  0  0" xyz="0.000000 0.011461 -0.000000"/>
      <inertia ixx="1.5e+10" ixy="0" ixz="0" iyy="2.6e+10" iyz="0" izz="1.5e+10"/>
    </inertial>
    <collision>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/flw.stl"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/flw.stl"/>
      </geometry>
    </visual>
  </link>
  <link name="frw">
    <inertial>
      <mass value="10"/>
      <origin rpy="0  0  0" xyz="0 -0.011461 0"/>
      <inertia ixx="1.5e+10" ixy="0" ixz="0" iyy="2.6e+10" iyz="0" izz="1.5e+10"/>
    </inertial>
    <collision>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/frw.stl"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/frw.stl"/>
      </geometry>
    </visual>
  </link>
  <link name="caster">
    <inertial>
      <mass value="5"/>
      <origin rpy="0  0  0" xyz="-0.000003 -0.000001 0.000003"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <collision>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/caster.stl"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0  0  0" xyz="0  0  0"/>
      <geometry>
        <mesh filename="package://autolabor_description/meshes/3d_model_modify/caster.stl"/>
      </geometry>
    </visual>
  </link>
  <link name="camera">
    <inertial>
      <mass value="0.001"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.000166667" iyy="0.000166667" izz="0.000166667" ixy="0" ixz="0" iyz="0"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>
  </link>
  <link name="ray">
    <inertial>
      <mass value="0.001"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.000166667" iyy="0.000166667" izz="0.000166667" ixy="0" ixz="0" iyz="0"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </visual>
  </link>

  <transmission name="tran1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="frjoint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor1">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
  <transmission name="tran2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="fljoint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="motor2">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>


  <gazebo reference="camera">
    <sensor type="camera" name="camera">
      <update_rate>60.0</update_rate>
      <camera>
        <horizontal_fov>1.5</horizontal_fov>
        <image>
          <width>1080</width>
          <height>720</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.1</near>
          <far>10</far>
        </clip>
      </camera>
      <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <cameraName>camera1</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>camera</frameName>
      </plugin>
    </sensor>
  </gazebo>
  <gazebo reference="ray">
    <sensor type="gpu_ray" name="ray">
      <update_rate>60.0</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>1000</samples>
            <resolution>1</resolution>
            <min_angle>-3.1415</min_angle>
            <max_angle>3.1415</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.1</min>
          <max>10</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin name="ray_controller" filename="libgazebo_ros_gpu_laser.so">
        <topicName>scan</topicName>
        <frameName>ray</frameName>
      </plugin>
    </sensor>
  </gazebo>
